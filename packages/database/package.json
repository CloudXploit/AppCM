    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@cm-diagnostics/eslint-config": "*",
    "@cm-diagnostics/typescript-config": "*",
    "@types/node": "^24.0.10",
    "prisma": "^6.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.2.0",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@cm-diagnostics/eslint-config": "*",
    "@cm-diagnostics/typescript-config": "*",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.0.10",
    "prisma": "^6.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.0.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} prisma/seed.ts"
